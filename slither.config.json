{
  "detectors_to_exclude": [
    "naming-convention",
    "solc-version"
  ],
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "filter_paths": [
    "node_modules",
    "contracts/mocks"
  ],
  "compile_force_framework": "hardhat",
  "solc_remaps": [
    "@openzeppelin/=node_modules/@openzeppelin/"
  ],
  "printers_to_run": [
    "contract-summary",
    "function-summary"
  ]
}
